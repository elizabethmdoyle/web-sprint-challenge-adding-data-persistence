{
  "name": "testing-apps",
  "version": "0.0.1",
  "scripts": {
    "dev": "fkill :3000 -s && npm run frontend",
    "frontend": "webpack serve --open",
    "server": "nodemon index.js",
    "ketchup": "git fetch && git reset --hard origin/lecture",
    "start": "node index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --forceExit"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "2.0.6",
    "concurrently": "7.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.10.0",
    "eslint-plugin-react": "7.29.3",
    "fkill-cli": "7.1.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.5.1",
    "msw": "0.39.1",
    "webpack": "^5.91.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "knex": "^2.4.2",
    "knex-cleaner": "^1.3.1",
    "sqlite3": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizabethmdoyle/web-sprint-challenge-adding-data-persistence.git"
  }
}
